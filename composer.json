{
    "type": "project",
    "description": "E-commerce marketplaces using the Symfony framework, Docker, PHP and PostgreSQL",
    "keywords": ["framework", "symfony", "symfony-7", "php", "dev", "blog", "tailwindcss",
        "marketplace", "shop", "store", "ecommerce", "online-shop", "php", "php-8.3",
        "postgresql", "posrgres", "javascript", "webpack", "docker", "redis"],
    "homepage": "https://github.com/alexfer/rgfly",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Sh",
            "email": "alexandershtyher@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "ext-zlib": "*",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "dompdf/dompdf": "^3.0",
        "elasticsearch/elasticsearch": "^8.14",
        "endroid/pdf": "^1.5",
        "knplabs/knp-paginator-bundle": "^6.4",
        "liip/imagine-bundle": "^2.12",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.29.1",
        "predis/predis": "^2.2",
        "ramsey/uuid": "^4.7",
        "stof/doctrine-extensions-bundle": "^v1.11.0",
        "symfony/asset": "7.1.*",
        "symfony/cache": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/flex": "^v2.4.3",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/google-mailer": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/intl": "7.1.*",
        "symfony/lock": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/messenger": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bundle": "^v3.4.0",
        "symfony/notifier": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/rate-limiter": "7.1.*",
        "symfony/redis-messenger": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/uid": "7.1.*",
        "symfony/ux-twig-component": "^v2.13.3",
        "symfony/validator": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "7.1.*",
        "symfonycasts/reset-password-bundle": "^v1.21.0",
        "symfonycasts/verify-email-bundle": "^v1.17.0",
        "twig/extra-bundle": "^2.12|^v3.10.0",
        "twig/twig": "^2.12|^v3.10.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^v1.59.1",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*"
    }
}
