{% extends 'base.html.twig' %}

{% block title %}{{ 'text.contact_us.success'|trans }}{% endblock %}
{% set _message = app.flashes('success') %}
{% block body %}
    <h2>{{ 'text.contact_us.success'|trans }}</h2>
    <p class="text-slate-500 dark:text-slate-400">{{ 'text.contact_us.success.short_desc'|trans|raw }}</p>
    <hr class="w-48 h-1 mx-auto my-2 bg-slate-100 border-0 rounded md:my-5 dark:bg-slate-700">
    <p class="text-slate-500 dark:text-slate-400 text-center">{{ 'text.contact_us.success.long_desc'|trans }}</p>
{% endblock %}
